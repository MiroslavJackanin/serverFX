<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="278.0" prefWidth="391.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.SignUp">
   <children>
      <TextField fx:id="fname" layoutX="135.0" layoutY="22.0" onKeyReleased="#setFstname" />
      <TextField fx:id="lname" layoutX="135.0" layoutY="66.0" onKeyReleased="#setLstname" />
      <TextField fx:id="login" layoutX="135.0" layoutY="108.0" onKeyReleased="#setUsrlogin" />
      <TextField fx:id="pass" layoutX="135.0" layoutY="152.0" onKeyReleased="#setUsrpass" />
      <Label layoutX="57.0" layoutY="26.0" text="First Name" />
      <Label layoutX="58.0" layoutY="70.0" text="Last Name" />
      <Label layoutX="72.0" layoutY="112.0" text="Login" />
      <Label layoutX="62.0" layoutY="156.0" text="Password" />
      <Button fx:id="signup" layoutX="170.0" layoutY="224.0" mnemonicParsing="false" onAction="#signUp" text="Sign Up" />
      <Label fx:id="errorMessage" layoutX="182.0" layoutY="192.0" />
      <Button fx:id="goBack" layoutX="20.0" layoutY="224.0" mnemonicParsing="false" onAction="#goBack" text="Back" />
   </children>
</AnchorPane>
