<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
    <children>
        <Pane prefHeight="354.0" prefWidth="455.0">
            <children>
                <TextField fx:id="fieldUserLogin" layoutX="152.0" layoutY="88.0" onKeyReleased="#setLogin" />
                <PasswordField fx:id="fieldUserPass" layoutX="152.0" layoutY="152.0" onKeyReleased="#setPass" />
                <Label layoutX="91.0" layoutY="92.0" prefHeight="17.0" prefWidth="38.0" text="Login" />
                <Label layoutX="84.0" layoutY="156.0" prefHeight="17.0" prefWidth="62.0" text="Password" />
                <Button fx:id="btnLogIn" layoutX="249.0" layoutY="266.0" mnemonicParsing="false" onAction="#logIn" text="Log In" />
                <Label fx:id="errMessage" alignment="CENTER" layoutX="76.0" layoutY="200.0" prefHeight="17.0" prefWidth="298.0" text="ERROR MESSAGE" textAlignment="CENTER" />
            </children>
        </Pane>
    </children>
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
</GridPane>
